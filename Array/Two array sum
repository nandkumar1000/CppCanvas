#include<iostream>
using namespace std;
int main(){
    int arr1[]={1,34,545,6,7,78,8,88};
    int arr2[]={123,45,345,554,2343,42,23};
    int n=7;
    int p=7;
    int sum1=0;
    int sum2=0;
    for(int i=0;i<=n;i++){
        cout<<arr1[i]<<endl;
        sum1+=arr1[i];
       // cout<<sum1<<endl;
    }
    for(int j=0;j<=p;j++){
        cout<<arr2[j]<<endl;
        sum2+=arr2[j];
        //cout<<sum2<<endl;
    }
    cout<<sum1<< " "<<"  "<<sum2<<" "<<sum1+sum2<<endl;
    return 0;
}