#include<iostream>
using namespace std;
int sum(int a,int b);
    
int main(){
    int num1,num2;
    cout<<"enter the value of num1"<<endl;
    cout<<"enter the value of num2"<<endl;
    cin>>num1;
    cin>>num2;
    cout<<"the sum is  "<<sum(num1,num2)<<endl;
    return 0;
    
}
int sum(int a,int b){
    int c=a+b;
    return c;
}
